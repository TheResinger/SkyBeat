<div class="card border-primary">
  <div class="card-body">
    <h5 class="card-title text-center"><span>{{songName}}</span> - <span>{{artistName}}</span></h5>
    <h6 class="card-title border-dark border-bottom text-center">{{genre}}</h6>
    <div class="row">
      <div class="col-12">
        <p class="card-text">{{body}}</p>
        <br>
        {{#goldstars rating}}
          <span>{{> goldstar}}</span>
        {{/goldstars}}
      </div>
    </div>
  </div>
</div>
<br>